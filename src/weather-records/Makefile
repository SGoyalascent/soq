CC = gcc
CFLAGS = -std=c99 -O0 -Wall -Werror -g -pedantic
CFLAGS = -std=c99 -O3 -Wall -Werror -g -pedantic -Wmissing-prototypes -Wstrict-prototypes -Wextra

PROG1 = climate
PROG2 = climate-v2
PROG3 = climate-v3
PROGS = ${PROG1} ${PROG2} ${PROG3}

all: ${PROGS}

clean:
	rm -f ${PROGS}
	rm -rf *.dSYM

